    "spellcheck": "cspell '**/*.md' '**/*.yml' --config ../cspell.json",
    "start": "next start",
    "storybook": "start-storybook -p 6006",
    "test": "cross-env-shell DEBUG_PRINT_LIMIT=50000 jest",
    "typecheck": "tsc --noemit",
    "typecheck:cypress": "tsc --noemit --project cypress/tsconfig.json",
    "webflow:sync": "node ./src/scripts/webflow/fundingSync.mjs --sync"
