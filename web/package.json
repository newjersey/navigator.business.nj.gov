    "gray-matter": "4.0.3",
    "lodash.orderby": "4.6.0",
    "markdown-truncate": "1.0.4",
    "netlify-cms-app": "2.15.66",
    "next": "12.0.10",
    "next-seo": "5.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-tabs": "3.2.3",
    "react-transition-group": "4.4.2",
    "remark": "12.0.1",
    "remark-html": "13.0.2",
    "remark-react": "8.0.0",
