{
  "name": "@businessnjgovnavigator/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "airtable:dump": "node src/scripts/airtableDump.js",
    "airtable:fetch-opps": "node src/scripts/airtableOpportunityDump.js",
    "build": "yarn next build && yarn next export",
    "build-storybook": "build-storybook",
    "clean": "rimraf .next public/vendor",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --record false",
    "cypress:run:feature": "cypress run --record false --env SUITE=feature",
    "cypress:run:ci": "cypress run --record false --env SUITE=ci",
    "deploy:storybook": "storybook-to-aws-s3 --out=.storybook/build",
    "dev": "next dev",
    "eslint": "eslint",
    "fences": "gf",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --max-warnings 0 --fix",
    "prepare:deploy": "next export",
    "prettier": "prettier --write . --ignore-path=./.eslintignore",
    "prettier:check": "prettier . --check --ignore-path=./.eslintignore",
    "run:napa": "napa",
    "start": "next start",
    "storybook": "start-storybook -p 6006",
    "test": "cross-env-shell DEBUG_PRINT_LIMIT=50000 jest",
    "test:ci": "cross-env-shell DEBUG_PRINT_LIMIT=50000 jest --reporters=jest-junit --outputFile=../coverage/test_results/web-jest.xml --runInBand --logHeapUsage --collectCoverage=true --coverageDirectory=../coverage/web/",
    "typecheck": "tsc --noemit",
    "typecheck:cypress": "tsc --noemit --project cypress/tsconfig.json"
  },
  "prettier": {
    "printWidth": 110,
    "endOfLine": "auto"
  },
  "dependencies": {
    "@aws-amplify/auth": "4.4.1",
