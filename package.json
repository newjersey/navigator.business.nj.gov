    "tsconfig-paths-webpack-plugin": "3.5.2",
    "typescript": "4.5.2",
    "webpack": "5.62.1",
    "webpack-node-externals": "3.0.0",
    "wiremock": "2.29.0",
    "yarn-workspace-sync-deps": "0.3.2"
  },
  "workspaces": {
    "packages": [
      "api",
      "content",
      "shared",
      "web"
    ]
  },
  "engines": {
    "node": ">=14.18.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newjersey/navigator.business.nj.gov.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/newjersey/navigator.business.nj.gov/issues"
  },
  "homepage": "https://github.com/newjersey/navigator.business.nj.gov#readme",
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --fix ",
    "*.{js,ts,tsx,css,md}": "prettier --write"
  },
  "prettier": {
    "printWidth": 110,
    "endOfLine": "auto"
  },
  "jest-junit": {
    "outputDirectory": "./coverage/test_results",
    "uniqueOutputName": "true"
  },
  "release": {
    "baseBranch": "main",
    "plugins": [
      "@semantic-release/commit-analyzer",
      {
        "preset": "angular",
        "parserOpts": {
          "noteKeywords": [
            "BREAKING CHANGE",
            "BREAKING CHANGES"
          ]
        }
      },
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "angular"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/changelog",
      [
        "@semantic-release/github",
        {
          "successComment": false
        }
      ],
      "@semantic-release/git"
    ]
  },
  "packageManager": "yarn@3.1.0"
}
