{
  "name": "@businessnjgovnavigator/root",
  "version": "1.2.0",
  "description": "This is the development repository for the work-in-progress business dashboard from the New Jersey Office of Innovation. For info on the existing [Business.NJ.gov](https://business.nj.gov) site, please see the [bottom of this document](https://github.com/newjersey/navigator.business.nj.gov#businessnjgov)",
  "main": "index.js",
  "private": true,
  "scripts": {
    "build": "yarn verify:node && lerna run --stream build",
    "build:clean": "yarn clean && yarn build",
    "clean": "lerna run --stream clean",
    "fences": "lerna run --stream fences",
    "lint": "lerna run --stream lint",
    "lint:fix": "lerna run lint:fix",
    "lint:staged": "lint-staged",
    "postinstall": "yarn workspace @businessnjgovnavigator/api install-dynamo-local && yarn workspace @businessnjgovnavigator/web run:napa",
    "preinstall": "npm install -g @aws-amplify/cli@7.3.6",
    "prepare": "husky install",
    "prettier": "prettier --write . --ignore-path=./.eslintignore",
    "prettier:check": "lerna run --stream prettier:check",
    "start:dev": "lerna run  --stream dev",
    "spellcheck": "lerna run --stream spellcheck",
    "sync:deps": "yarnw-sync-deps --fix",
    "test": "jest --projects api shared web",
    "test:ci": "jest --reporters=jest-junit --outputFile=../coverage/test_results/all-jest.xml --runInBand --logHeapUsage --collectCoverage=true --coverageDirectory=../coverage/ --projects api shared web",
    "typecheck": "lerna run --stream typecheck",
    "verify:node": "check-node-version --node $(cat .nvmrc) --npm 6.14"
  },
  "dependencies": {
    "@aws-amplify/auth": "4.3.14",
    "@aws-amplify/core": "4.3.6",
    "@aws-amplify/ui-react": "1.2.24",
    "@businessnjgovnavigator/shared": "*",
    "@emotion/react": "11.6.0",
    "@emotion/styled": "11.6.0",
    "@mui/lab": "5.0.0-alpha.54",
    "@mui/material": "5.1.0",
    "@mui/styles": "5.1.0",
    "@next/env": "12.0.3",
    "@storybook/addon-outline": "6.4.0-alpha.27",
    "airtable": "0.11.1",
    "aws-lambda": "1.0.7",
    "axios": "0.24.0",
    "babel-jest": "27.3.1",
    "body-parser": "1.19.0",
    "broken-link-checker": "0.7.8",
    "cors": "2.8.5",
    "dayjs": "1.10.7",
