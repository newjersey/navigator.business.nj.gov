  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,md,scss,yml}": "prettier --write"
  },
  "prettier": {
    "printWidth": 110,
    "endOfLine": "auto"
  },
  "jest": {
    "coverageReporters": [
      "json-summary",
      "text"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "verbose": true
  },
  "jest-junit": {
    "outputDirectory": "./coverage/test_results",
    "uniqueOutputName": "false",
    "outputName": "all-jest.xml",
    "addFileAttribute": "true"
  },
  "release": {
    "baseBranch": "main",
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES"
            ]
          }
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "angular"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/changelog",
      [
        "@semantic-release/github",
        {
          "successComment": false
        }
      ],
      "@semantic-release/git"
    ]
  },
  "packageManager": "yarn@3.5.0",
  "resolutions": {
    "trim@0.0.1": "0.0.3",
    "postcss": "8.4.21",
    "glob-parent": "6.0.2",
    "tar": "6.1.13",
