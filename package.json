  "engines": {
    "node": ">=14.19.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newjersey/navigator.business.nj.gov.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/newjersey/navigator.business.nj.gov/issues"
  },
  "homepage": "https://github.com/newjersey/navigator.business.nj.gov#readme",
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --fix ",
    "*.{js,ts,tsx,css,md}": "prettier --write"
  },
  "prettier": {
    "printWidth": 110,
    "endOfLine": "auto"
  },
  "jest-junit": {
    "outputDirectory": "./coverage/test_results",
    "uniqueOutputName": "true"
  },
  "release": {
    "baseBranch": "main",
    "plugins": [
      "@semantic-release/commit-analyzer",
      {
        "preset": "angular",
        "parserOpts": {
          "noteKeywords": [
            "BREAKING CHANGE",
            "BREAKING CHANGES"
          ]
        }
      },
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "angular"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/changelog",
      [
        "@semantic-release/github",
        {
          "successComment": false
        }
      ],
      "@semantic-release/git"
    ]
  },
  "packageManager": "yarn@3.2.0",
  "resolutions": {
    "trim@0.0.1": "0.0.3",
    "postcss": "8.4.6",
    "glob-parent": "6.0.2",
    "tar": "6.1.11",
    "ansi-regex": "5.0.1",
    "nanoid": "3.3.1"
  }
