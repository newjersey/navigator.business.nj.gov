{
  "name": "@businessnjgovnavigator/content",
  "version": "1.0.0",
  "description": "A content library for the business.nj.gov navigator",
  "private": true,
  "main": "src",
  "scripts": {
    "build": "node ./build.js",
    "clean": "rimraf lib",
    "prettier": "prettier --write . --ignore-path=./.eslintignore",
    "prettier:check": "prettier . --check --ignore-path=./.eslintignore",
    "spellcheck": "cspell -c ../cspell.json  '**/*.md'"
  },
  "prettier": {
    "printWidth": 110,
    "endOfLine": "auto"
  },
  "devDependencies": {
    "@types/node": "14.18.5",
    "cross-env": "7.0.3",
    "cspell": "5.14.0",
    "prettier": "2.5.1",
    "rimraf": "3.0.2"
  }
}
