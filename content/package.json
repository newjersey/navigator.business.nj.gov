{
  "name": "@businessnjgovnavigator/content",
  "version": "1.0.0",
  "description": "A content library for the business.nj.gov navigator",
  "private": true,
  "main": "src",
  "scripts": {
    "build": "node ./build.js",
    "clean": "rimraf lib",
    "dev": "yarn build",
    "prettier": "prettier --write . --ignore-path=../.eslintignore",
    "prettier:check": "prettier . --check --ignore-path=../.eslintignore",
    "spellcheck": "cspell '**/*.md' '**/*.json' --config ../cspell.json"
  },
  "devDependencies": {
    "@types/node": "20.19.7",
    "cross-env": "7.0.3",
    "cspell": "9.2.0",
    "prettier": "3.6.2",
    "rimraf": "6.0.1"
  }
}
