{
  "env": {
    "es2021": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {},
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "@typescript-eslint/eslint-plugin"],
  "rules": {
    "@typescript-eslint/no-empty-function": "off",
    "prefer-template": "error"
  }
}
