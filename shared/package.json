{
  "name": "@businessnjgovnavigator/shared",
  "version": "1.0.0",
  "description": "A shared library for the business.nj.gov navigator",
  "private": true,
  "main": "lib/shared/src/index.js",
  "files": [
    "lib"
  ],
  "typings": "./lib/shared/src/index.d.ts",
  "scripts": {
    "build": "tsc --build",
    "clean": "rimraf lib",
    "dev": "yarn build",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --max-warnings 0 --fix",
    "snapshot:update": "jest --updateSnapshot",
    "prettier": "prettier --write . --ignore-path=./.eslintignore",
    "prettier:check": "prettier . --check --ignore-path=./.eslintignore",
    "test": "jest",
    "test:ci": "jest --reporters=jest-junit --outputFile=../coverage/test_results/shared-jest.xml --runInBand --logHeapUsage --collectCoverage=true --coverageDirectory=../coverage/shared/",
    "typecheck": "tsc --noemit"
  },
  "prettier": {
    "printWidth": 110,
    "endOfLine": "auto"
  },
  "dependencies": {
    "@businessnjgovnavigator/content": "*",
    "dayjs": "1.11.7",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@jest/types": "29.5.0",
    "@types/jest": "29.4.0",
    "@types/node": "16.18.13",
    "@types/uuid": "9.0.1",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.54.0",
    "cross-env": "7.0.3",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-unicorn": "45.0.2",
    "jest": "29.5.0",
    "jest-junit": "15.0.0",
    "prettier": "2.8.4",
    "rimraf": "4.1.2",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.2",
    "typescript": "4.9.5"
  },
  "jest-junit": {
    "outputDirectory": "../coverage/test_results",
    "uniqueOutputName": "true"
  }
}
