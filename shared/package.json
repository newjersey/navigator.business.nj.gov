{
  "name": "@businessnjgovnavigator/shared",
  "version": "1.0.0",
  "description": "A shared library for the business.nj.gov navigator",
  "private": true,
  "main": "lib/shared/src/index.js",
  "files": [
    "lib"
  ],
  "typings": "./lib/shared/src/index.d.ts",
  "scripts": {
    "build": "tsc --build",
    "clean": "rimraf lib",
    "dev": "yarn build",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --max-warnings 0 --fix",
    "snapshot:update": "jest --updateSnapshot",
    "prettier": "prettier --write . --ignore-path=./.eslintignore",
    "prettier:check": "prettier . --check --ignore-path=./.eslintignore",
    "test": "jest",
    "typecheck": "tsc --noemit"
  },
  "prettier": {
    "printWidth": 110,
    "endOfLine": "auto"
  },
  "dependencies": {
    "@businessnjgovnavigator/content": "*",
    "dayjs": "1.11.9",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@jest/types": "29.6.3",
    "@types/jest": "29.5.5",
    "@types/node": "16.18.52",
    "@types/uuid": "9.0.4",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "cross-env": "7.0.3",
    "eslint": "8.49.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-unicorn": "48.0.1",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "prettier": "2.8.8",
    "rimraf": "5.0.1",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "4.9.5"
  },
  "jest-junit": {
    "outputDirectory": "../coverage/test_results",
    "outputName": "shared-jest.xml",
    "uniqueOutputName": "false",
    "addFileAttribute": "true"
  }
}
