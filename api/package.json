    "prettier-plugin-organize-imports": "4.1.0",
    "rimraf": "6.0.1",
    "serverless": "4.17.1",
    "serverless-dynamodb": "0.2.56",
    "serverless-offline": "14.4.0",
    "serverless-offline-ssm": "6.2.0",
    "serverless-prune-plugin": "2.1.0",
    "supertest": "6.3.4",
    "ts-jest": "29.4.4",
