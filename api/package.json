{
  "name": "@businessnjgovnavigator/api",
  "version": "1.0.0",
  "main": "serverless.ts",
  "private": true,
  "scripts": {
    "build": "sls package",
    "clean": "rimraf .serverless .webpack",
    "deploy": "sls deploy",
    "dev": "yarn wiremock --root-dir ./wiremock --port 9000 & yarn start",
    "dependency-check": "yarn depcruise --config .dependency-cruiser.js src",
    "install-dynamo-local": "sls dynamodb install",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --max-warnings 0 --fix",
    "prettier": "prettier --write . --ignore-path=./.eslintignore",
    "prettier:check": "prettier . --check --ignore-path=./.eslintignore",
    "serverless": "serverless",
    "sls": "serverless",
    "start": "sls offline start",
    "invokeNewsletter": "IS_OFFLINE=true sls invoke local --function govDelivery",
    "start:wiremock": "wiremock --root-dir ./wiremock --port 9000",
    "start:wiremock:with-port": "wiremock --root-dir ./wiremock",
    "test": "jest",
    "test:ci": "jest --reporters=jest-junit --outputFile=../coverage/test_results/api-jest.xml --runInBand --logHeapUsage --collectCoverage=true --coverageDirectory=../coverage/api/",
    "typecheck": "tsc --noemit"
  },
  "engines": {
    "node": ">=16.18.0"
  },
  "prettier": {
    "printWidth": 110,
    "endOfLine": "auto"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "3.204.0",
    "@aws-sdk/client-dynamodb": "3.204.0",
    "@aws-sdk/client-s3": "3.204.0",
    "@aws-sdk/lib-dynamodb": "3.204.0",
    "@aws-sdk/smithy-client": "3.201.0",
    "@aws-sdk/types": "3.201.0",
    "@aws-sdk/util-dynamodb": "3.204.0",
    "@businessnjgovnavigator/shared": "*",
    "@serverless/typescript": "3.21.0",
    "airtable": "0.11.5",
    "axios": "0.27.2",
    "body-parser": "1.20.1",
    "cors": "2.8.5",
    "dayjs": "1.11.6",
    "dedent": "0.7.0",
    "express": "4.18.2",
    "helmet": "6.0.0",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "serverless-http": "3.1.0",
    "simple-oauth2": "5.0.0",
    "uuid": "9.0.0",
    "winston": "3.8.2",
    "winston-cloudwatch": "6.1.1",
    "xml2js": "0.4.23"
  },
  "devDependencies": {
    "@businessnjgovnavigator/shared": "*",
    "@jest/types": "29.2.1",
    "@types/cors": "2.8.12",
    "@types/dedent": "0.7.0",
    "@types/express": "4.17.14",
    "@types/jest": "29.2.2",
    "@types/jsonwebtoken": "8.5.9",
    "@types/lodash": "4.14.188",
    "@types/node": "16.18.3",
    "@types/simple-oauth2": "4.1.1",
    "@types/supertest": "2.0.12",
    "@types/webpack-node-externals": "2.5.3",
    "@types/xml2js": "0.4.11",
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "5.42.0",
    "copy-webpack-plugin": "11.0.0",
    "cross-env": "7.0.3",
    "dependency-cruiser": "11.18.0",
    "eslint": "8.27.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "27.1.4",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-unicorn": "44.0.2",
    "jest": "29.2.2",
    "jest-dynalite": "3.6.1",
    "jest-junit": "14.0.1",
    "prettier": "2.7.1",
    "prettier-plugin-organize-imports": "3.1.1",
    "rimraf": "3.0.2",
    "serverless": "3.24.0",
    "serverless-dynamodb-local": "0.2.40",
    "serverless-offline": "11.2.3",
    "serverless-offline-ssm": "6.2.0",
    "serverless-webpack": "5.10.0",
    "supertest": "6.3.1",
    "ts-jest": "29.0.3",
    "ts-loader": "9.4.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "tsconfig-paths-webpack-plugin": "4.0.0",
    "typescript": "4.8.4",
    "webpack": "5.74.0",
    "webpack-node-externals": "3.0.0",
    "wiremock": "2.35.0"
  },
  "jest-junit": {
    "outputDirectory": "../coverage/test_results",
    "uniqueOutputName": "true"
  }
}
