{
  "name": "@businessnjgovnavigator/api",
  "version": "1.0.0",
  "main": "serverless.ts",
  "private": "true",
  "scripts": {
    "build": "sls package",
    "deploy": "sls deploy",
    "dev": "npm run start",
    "fences": "gf",
    "install-dynamo-local": "sls dynamodb install",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --max-warnings 0 --fix",
    "postinstall": "npm run install-dynamo-local",
    "prettier": "prettier --write . --ignore-path=./.eslintignore",
    "prettier:check": "prettier . --check --ignore-path=./.eslintignore",
    "serverless": "serverless",
    "sls": "serverless",
    "start": "sls offline start",
    "start:wiremock": "wiremock --root-dir ./wiremock --port 9000",
    "start:wiremock:with-port": "wiremock --root-dir ./wiremock",
    "test": "jest",
    "test:ci": "jest --reporters=jest-junit --outputFile=../reports/test_results/api-jest.xml --runInBand --logHeapUsage --collectCoverage=true --coverageDirectory=../reports/coverage/api/",
    "typecheck": "tsc --noemit"
  },
  "engines": {
    "node": ">=14.17.5"
  },
  "prettier": {
    "printWidth": 110,
    "endOfLine": "auto"
  },
  "dependencies": {
    "aws-lambda": "1.0.6",
    "axios": "0.21.4",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "dayjs": "1.10.7",
    "express": "4.17.1",
    "jsonwebtoken": "8.5.1",
    "serverless-http": "2.7.0",
    "serverless-offline": "8.2.0",
    "uuid": "8.3.2",
    "winston": "3.3.3",
    "winston-cloudwatch": "3.1.0",
    "xml2js": "0.4.23"
  },
  "devDependencies": {
    "@serverless/typescript": "2.61.0",
    "@types/aws-lambda": "8.10.84",
    "@types/copy-webpack-plugin": "8.0.1",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/jasmine": "3.10.0",
    "@types/jest": "27.0.2",
    "@types/jsonwebtoken": "8.5.5",
    "@types/node": "14.17.22",
    "@types/supertest": "2.0.11",
    "@types/uuid": "8.3.1",
    "@types/webpack-node-externals": "2.5.3",
    "@types/xml2js": "0.4.9",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "aws-sdk": "2.996.0",
    "copy-webpack-plugin": "9.0.1",
    "cross-env": "7.0.3",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "good-fences": "1.0.0",
    "jasmine": "3.10.0",
    "jest": "27.3.1",
    "jest-dynalite": "3.4.4",
    "jest-junit": "13.0.0",
    "prettier": "2.4.1",
    "serverless": "2.62.0",
    "serverless-dynamodb-local": "0.2.40",
    "serverless-offline-ssm": "5.2.0",
    "serverless-webpack": "5.5.4",
    "supertest": "6.1.6",
    "ts-jest": "27.0.7",
    "ts-loader": "9.2.6",
    "ts-node": "10.3.0",
    "tsconfig-paths": "3.11.0",
    "tsconfig-paths-webpack-plugin": "3.5.1",
    "typescript": "4.4.4",
    "webpack": "5.58.2",
    "webpack-node-externals": "3.0.0",
    "wiremock": "2.29.0"
  },
  "jest-junit": {
    "outputDirectory": "../reports/test_results",
    "uniqueOutputName": "true"
  }
}
