{
  "name": "@businessnjgovnavigator/api",
  "version": "1.0.0",
  "main": "serverless.ts",
  "private": true,
  "scripts": {
    "build": "sls package",
    "clean": "rimraf .serverless",
    "deploy": "sls deploy",
    "dev": "yarn wiremock --root-dir ./wiremock --port 9000 & yarn start:hotReload",
    "dependency-check": "yarn depcruise --config .dependency-cruiser.js src",
    "install-dynamo-local": "./scripts/dynamodb-local.sh",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --max-warnings 0 --fix",
    "prettier": "prettier --write . --ignore-path=./.eslintignore",
    "prettier:check": "prettier . --check --ignore-path=./.eslintignore",
    "serverless": "serverless",
    "sls": "serverless",
    "start": "sls offline start",
    "start:hotReload": "sls offline start --reloadHandler",
    "invokeNewsletter": "IS_OFFLINE=true sls invoke local --function govDelivery",
    "start:wiremock": "wiremock --root-dir ./wiremock --port 9000",
    "start:wiremock:with-port": "wiremock --root-dir ./wiremock",
    "test": "jest",
    "typecheck": "tsc --noemit"
  },
  "engines": {
    "node": ">=22.16.0"
  },
  "dependencies": {
    "@aws-crypto/client-node": "4.2.1",
    "@aws-sdk/client-cloudwatch": "3.864.0",
    "@aws-sdk/client-cloudwatch-logs": "3.844.0",
    "@aws-sdk/client-dynamodb": "3.844.0",
    "@aws-sdk/client-s3": "3.844.0",
    "@aws-sdk/client-sns": "3.876.0",
    "@aws-sdk/client-ssm": "3.844.0",
    "@aws-sdk/lib-dynamodb": "3.844.0",
    "@aws-sdk/smithy-client": "3.374.0",
    "@aws-sdk/types": "3.840.0",
    "@aws-sdk/util-base64-node": "3.209.0",
    "@aws-sdk/util-dynamodb": "3.844.0",
    "@businessnjgovnavigator/shared": "*",
    "@serverless/typescript": "3.38.0",
    "@smithy/node-http-handler": "2.5.0",
    "airtable": "0.12.2",
    "axios": "1.12.0",
    "body-parser": "1.20.3",
    "cors": "2.8.5",
    "dayjs": "1.11.13",
    "dedent": "1.6.0",
    "express": "4.21.2",
    "helmet": "7.2.0",
    "http-status-codes": "2.3.0",
    "jsonwebtoken": "9.0.2",
    "lodash": "4.17.21",
    "serverless-http": "3.2.0",
    "simple-oauth2": "5.1.0",
    "uuid": "9.0.1",
    "winston": "3.17.0",
    "winston-cloudwatch": "6.3.0",
    "xml2js": "0.6.2"
  },
  "devDependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.777.0",
    "@businessnjgovnavigator/shared": "*",
    "@jest/types": "29.6.3",
    "@types/cors": "2.8.19",
    "@types/dedent": "0.7.2",
    "@types/express": "5.0.0",
    "@types/jest": "29.5.14",
    "@types/jsonwebtoken": "9.0.10",
    "@types/lodash": "4.17.20",
    "@types/node": "20.19.17",
    "@types/simple-oauth2": "5.0.7",
    "@types/supertest": "6.0.3",
    "@types/xml2js": "0.4.14",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "cross-env": "7.0.3",
    "dependency-cruiser": "16.10.4",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.2",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-unicorn": "51.0.1",
    "jest": "29.7.0",
    "jest-dynalite": "3.6.1",
    "jest-junit": "16.0.0",
