{
  "name": "@businessnjgovnavigator/api",
  "version": "1.0.0",
  "main": "serverless.ts",
  "private": true,
  "scripts": {
    "build": "sls package",
    "clean": "rimraf .serverless .webpack",
    "deploy": "sls deploy",
    "dev": "yarn wiremock --root-dir ./wiremock --port 9000 & yarn start",
    "fences": "gf",
    "install-dynamo-local": "sls dynamodb install",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --max-warnings 0 --fix",
    "prettier": "prettier --write . --ignore-path=./.eslintignore",
    "prettier:check": "prettier . --check --ignore-path=./.eslintignore",
    "serverless": "serverless",
    "sls": "serverless",
    "start": "sls offline start",
    "invokeNewsletter": "IS_OFFLINE=true sls invoke local --function govDelivery",
    "start:wiremock": "wiremock --root-dir ./wiremock --port 9000",
    "start:wiremock:with-port": "wiremock --root-dir ./wiremock",
    "test": "jest",
    "test:ci": "jest --reporters=jest-junit --outputFile=../coverage/test_results/api-jest.xml --runInBand --logHeapUsage --collectCoverage=true --coverageDirectory=../coverage/api/",
    "typecheck": "tsc --noemit"
  },
  "engines": {
    "node": ">=14.19.1"
  },
  "prettier": {
    "printWidth": 110,
    "endOfLine": "auto"
  },
  "dependencies": {
    "airtable": "0.11.2",
    "axios": "0.26.1",
    "body-parser": "1.19.2",
    "cors": "2.8.5",
    "dayjs": "1.11.0",
    "dedent": "0.7.0",
    "express": "4.17.3",
    "jsonwebtoken": "8.5.1",
    "serverless-http": "2.7.0",
    "simple-oauth2": "4.3.0",
    "winston": "3.6.0",
    "winston-cloudwatch": "3.1.1",
    "xml2js": "0.4.23"
  },
  "devDependencies": {
    "@businessnjgovnavigator/shared": "*",
    "@jest/types": "27.5.1",
    "@serverless/typescript": "3.8.0",
    "@types/cors": "2.8.12",
    "@types/dedent": "0.7.0",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.1",
    "@types/jsonwebtoken": "8.5.8",
    "@types/node": "14.18.12",
    "@types/simple-oauth2": "4.1.1",
    "@types/supertest": "2.0.12",
    "@types/webpack-node-externals": "2.5.3",
    "@types/xml2js": "0.4.9",
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "aws-sdk": "2.1096.0",
    "copy-webpack-plugin": "10.2.4",
    "cross-env": "7.0.3",
    "eslint": "8.11.0",
    "eslint-config-prettier": "8.5.0",
    "good-fences": "1.0.2",
    "jest": "27.5.1",
    "jest-dynalite": "3.4.4",
    "jest-junit": "13.0.0",
    "prettier": "2.6.1",
    "prettier-plugin-organize-imports": "2.3.4",
    "rimraf": "3.0.2",
    "serverless": "3.10.0",
    "serverless-dynamodb-local": "0.2.40",
    "serverless-offline": "8.5.0",
    "serverless-offline-ssm": "6.2.0",
    "serverless-plugin-monorepo": "0.11.0",
    "serverless-webpack": "5.6.1",
    "supertest": "6.2.2",
    "ts-jest": "27.1.4",
    "ts-loader": "9.2.8",
    "ts-node": "10.7.0",
    "tsconfig-paths": "3.14.0",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "typescript": "4.6.3",
    "webpack": "5.70.0",
    "webpack-node-externals": "3.0.0",
    "wiremock": "2.32.0"
  },
  "jest-junit": {
    "outputDirectory": "../coverage/test_results",
    "uniqueOutputName": "true"
  }
}
