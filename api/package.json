{
  "name": "@businessnjgovnavigator/api",
  "version": "1.0.0",
  "main": "serverless.ts",
  "private": true,
  "scripts": {
    "build": "sls package",
    "clean": "rimraf .serverless .webpack",
    "deploy": "sls deploy",
    "dev": "yarn wiremock --root-dir ./wiremock --port 9000 & yarn start",
    "dependency-check": "yarn depcruise --config .dependency-cruiser.js src",
    "install-dynamo-local": "./scripts/dynamodb-local.sh",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --max-warnings 0 --fix",
    "prettier": "prettier --write . --ignore-path=./.eslintignore",
    "prettier:check": "prettier . --check --ignore-path=./.eslintignore",
    "serverless": "serverless",
    "sls": "serverless",
    "start": "sls offline start",
    "invokeNewsletter": "IS_OFFLINE=true sls invoke local --function govDelivery",
    "start:wiremock": "wiremock --root-dir ./wiremock --port 9000",
    "start:wiremock:with-port": "wiremock --root-dir ./wiremock",
    "test": "jest",
    "typecheck": "tsc --noemit"
  },
  "engines": {
    "node": ">=18.16.0"
  },
  "prettier": {
    "printWidth": 110,
    "endOfLine": "auto"
  },
  "dependencies": {
    "@aws-crypto/client-node": "3.2.2",
    "@aws-sdk/client-cloudwatch-logs": "3.369.0",
    "@aws-sdk/client-dynamodb": "3.369.0",
    "@aws-sdk/client-s3": "3.369.0",
    "@aws-sdk/lib-dynamodb": "3.369.0",
    "@aws-sdk/smithy-client": "3.369.0",
    "@aws-sdk/types": "3.369.0",
    "@aws-sdk/util-base64-node": "3.209.0",
    "@aws-sdk/util-dynamodb": "3.369.0",
    "@businessnjgovnavigator/shared": "*",
    "@serverless/typescript": "3.30.1",
    "airtable": "0.12.1",
    "axios": "0.27.2",
    "body-parser": "1.20.2",
    "cors": "2.8.5",
    "dayjs": "1.11.9",
    "dedent": "0.7.0",
    "express": "4.18.2",
    "helmet": "6.2.0",
    "jsonwebtoken": "9.0.1",
    "lodash": "4.17.21",
    "serverless-http": "3.2.0",
    "simple-oauth2": "5.0.0",
    "uuid": "9.0.0",
    "winston": "3.10.0",
    "winston-cloudwatch": "6.1.1",
    "xml2js": "0.6.0"
  },
  "devDependencies": {
    "@businessnjgovnavigator/shared": "*",
    "@jest/types": "29.6.1",
    "@types/cors": "2.8.13",
    "@types/dedent": "0.7.0",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.2",
    "@types/jsonwebtoken": "9.0.2",
    "@types/lodash": "4.14.195",
    "@types/node": "16.18.40",
    "@types/simple-oauth2": "5.0.4",
    "@types/supertest": "2.0.12",
    "@types/webpack-node-externals": "3.0.0",
    "@types/xml2js": "0.4.11",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "copy-webpack-plugin": "11.0.0",
    "cross-env": "7.0.3",
    "dependency-cruiser": "13.1.3",
    "eslint": "8.44.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-jest": "27.2.2",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-unicorn": "46.0.1",
    "jest": "29.6.1",
    "jest-dynalite": "3.6.1",
    "jest-junit": "16.0.0",
    "prettier": "2.8.8",
    "prettier-plugin-organize-imports": "3.2.2",
    "rimraf": "5.0.1",
    "serverless": "3.33.0",
    "serverless-dynamodb": "0.2.42",
    "serverless-offline": "12.0.4",
    "serverless-offline-ssm": "6.2.0",
    "serverless-webpack": "5.11.0",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "tsconfig-paths-webpack-plugin": "4.1.0",
    "typescript": "4.9.5",
    "webpack": "5.88.1",
    "webpack-node-externals": "3.0.0",
    "wiremock": "2.35.0"
  },
  "jest-junit": {
    "outputDirectory": "../coverage/test_results",
    "outputName": "api-jest.xml",
    "uniqueOutputName": "false",
    "addFileAttribute": "true"
  }
}
