{
  "name": "@businessnjgovnavigator/api",
  "version": "1.0.0",
  "main": "serverless.ts",
  "private": true,
  "scripts": {
    "build": "sls package",
    "clean": "rimraf .serverless .webpack",
    "deploy": "sls deploy",
    "dev": "yarn wiremock --root-dir ./wiremock --port 9000 & yarn start",
    "fences": "gf",
    "install-dynamo-local": "sls dynamodb install",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --max-warnings 0 --fix",
    "prettier": "prettier --write . --ignore-path=./.eslintignore",
    "prettier:check": "prettier . --check --ignore-path=./.eslintignore",
    "serverless": "serverless",
    "sls": "serverless",
    "start": "sls offline start",
    "start:wiremock": "wiremock --root-dir ./wiremock --port 9000",
    "start:wiremock:with-port": "wiremock --root-dir ./wiremock",
    "test": "jest",
    "test:ci": "jest --reporters=jest-junit --outputFile=../coverage/test_results/api-jest.xml --runInBand --logHeapUsage --collectCoverage=true --coverageDirectory=../coverage/api/",
    "typecheck": "tsc --noemit"
  },
  "engines": {
    "node": ">=14.18.1"
  },
  "prettier": {
    "printWidth": 110,
    "endOfLine": "auto"
  },
  "dependencies": {
    "aws-lambda": "1.0.7",
    "axios": "0.24.0",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "dayjs": "1.10.7",
    "express": "4.17.1",
    "jsonwebtoken": "8.5.1",
    "serverless-http": "2.7.0",
    "uuid": "8.3.2",
    "winston": "3.3.3",
    "winston-cloudwatch": "3.1.1",
    "xml2js": "0.4.23"
  },
  "devDependencies": {
    "@businessnjgovnavigator/shared": "*",
    "@jest/types": "27.2.5",
    "@serverless/typescript": "2.67.0",
    "@types/aws-lambda": "8.10.85",
    "@types/copy-webpack-plugin": "8.0.1",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/jasmine": "3.10.2",
    "@types/jest": "27.0.3",
    "@types/jsonwebtoken": "8.5.6",
    "@types/node": "14.17.34",
    "@types/supertest": "2.0.11",
    "@types/uuid": "8.3.3",
    "@types/webpack-node-externals": "2.5.3",
    "@types/xml2js": "0.4.9",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "aws-sdk": "2.1030.0",
    "copy-webpack-plugin": "10.0.0",
    "cross-env": "7.0.3",
    "eslint": "8.3.0",
    "eslint-config-prettier": "8.3.0",
    "good-fences": "1.0.1",
    "jasmine": "3.10.0",
    "jest": "27.3.1",
    "jest-dynalite": "3.4.4",
    "jest-junit": "13.0.0",
    "prettier": "2.5.0",
    "rimraf": "3.0.2",
    "serverless": "2.67.0",
    "serverless-dynamodb-local": "0.2.40",
    "serverless-offline": "8.3.1",
    "serverless-offline-ssm": "5.2.0",
    "serverless-plugin-monorepo": "0.10.0",
    "serverless-webpack": "5.6.0",
    "supertest": "6.1.6",
    "ts-jest": "27.0.7",
    "ts-loader": "9.2.6",
    "ts-node": "10.4.0",
    "tsconfig-paths": "3.12.0",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "typescript": "4.5.2",
    "webpack": "5.64.4",
    "webpack-node-externals": "3.0.0",
    "wiremock": "2.29.0"
  },
  "jest-junit": {
    "outputDirectory": "../coverage/test_results",
    "uniqueOutputName": "true"
  }
}
