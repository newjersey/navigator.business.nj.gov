{
  "name": "businessnjgov-api",
  "version": "1.0.0",
  "main": "serverless.ts",
  "private": "true",
  "scripts": {
    "start": "sls offline start",
    "dev": "npm run start",
    "deploy": "sls deploy",
    "build": "sls package",
    "test": "jest",
    "prettier": "prettier --write . --ignore-path=./.eslintignore",
    "prettier:check": "prettier . --check --ignore-path=./.eslintignore",
    "lint": "eslint . --max-warnings 0 --ext .js,.ts",
    "lint:fix": "eslint . --max-warnings 0 --ext .js,.ts --fix",
    "install-dynamo-local": "sls dynamodb install",
    "postinstall": "npm run install-dynamo-local",
    "fences": "gf",
    "start:wiremock": "wiremock --root-dir ./wiremock --port 9000",
    "start:wiremock:with-port": "wiremock --root-dir ./wiremock",
    "sls": "serverless",
    "serverless": "serverless",
    "typecheck": "tsc --noemit"
  },
  "engines": {
    "node": ">=14.17.5"
  },
  "prettier": {
    "printWidth": 110,
    "endOfLine": "auto"
  },
  "dependencies": {
    "aws-lambda": "1.0.6",
    "axios": "0.21.1",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "dayjs": "1.10.6",
    "express": "4.17.1",
    "jsonwebtoken": "8.5.1",
    "serverless-http": "2.7.0",
    "serverless-offline": "8.0.0",
    "source-map-support": "0.5.19",
    "uuid": "8.3.2",
    "winston": "3.3.3",
    "winston-cloudwatch": "3.0.1",
    "xml2js": "0.4.23"
  },
  "devDependencies": {
    "@serverless/typescript": "2.55.0",
    "@types/aws-lambda": "8.10.83",
    "@types/copy-webpack-plugin": "8.0.1",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/jasmine": "3.8.2",
    "@types/jest": "27.0.1",
    "@types/jsonwebtoken": "8.5.5",
    "@types/node": "14.17.12",
    "@types/supertest": "2.0.11",
    "@types/uuid": "8.3.1",
    "@types/webpack-node-externals": "2.5.2",
    "@types/xml2js": "0.4.9",
    "@typescript-eslint/eslint-plugin": "4.29.2",
    "@typescript-eslint/parser": "4.29.2",
    "aws-sdk": "2.972.0",
    "copy-webpack-plugin": "9.0.1",
    "cross-env": "7.0.3",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "good-fences": "0.10.0",
    "jasmine": "3.9.0",
    "jest": "27.0.6",
    "jest-dynalite": "3.4.1",
    "json-schema-to-ts": "1.6.4",
    "prettier": "2.3.2",
    "serverless": "2.57.0",
    "serverless-dynamodb-local": "0.2.40",
    "serverless-webpack": "5.5.3",
    "supertest": "6.1.6",
    "ts-jest": "27.0.5",
    "ts-loader": "9.2.5",
    "ts-node": "10.2.1",
    "tsconfig-paths": "3.11.0",
    "tsconfig-paths-webpack-plugin": "3.5.1",
    "typescript": "4.3.5",
    "webpack": "5.51.1",
    "webpack-node-externals": "3.0.0",
    "wiremock": "2.29.0"
  }
}
