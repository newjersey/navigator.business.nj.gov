{
  "request": {
    "method": "POST",
    "url": "/TYTR_ACE_App/ProcessCertificate/businessClearance",
    "bodyPatterns": [
      {
        "matchesJsonPath": "$[?(@.taxpayerId == '777777777771')]"
      },
      {
        "matchesJsonPath": "$[?(@.taxpayerPin == '3889')]"
      }
    ]
  },
  "response": {
    "status": 200,
    "bodyFileName": "tax-clearance-success.json",
    "headers": {
      "Content-Type": "text/json"
    }
  }
}
